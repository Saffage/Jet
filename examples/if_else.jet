main :: () {
    # 'if-else' statement
    if false {
        $print("false")
    } else if true {
        $print("true")
    } else {
        $print("unreachable")
    }

    # 'if-else' expression (similar to statement)
    x := if false {
        10
    } else if true {
        20
    } else {
        30
    }
}
